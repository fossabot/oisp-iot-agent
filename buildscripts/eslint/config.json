{
	"extends": "eslint:recommended",
	"env": {
		"node": true
	},
	"parserOptions": { "ecmaVersion": "latest" },
	"rules": {
	    "no-console": "off",
	    "indent": ["error", 4, { "ArrayExpression": "first", "ObjectExpression": "first" }],
	    "brace-style": ["error", "1tbs"],
	    "space-before-blocks": "error",
	    "no-prototype-builtins": "off"
	}
}
